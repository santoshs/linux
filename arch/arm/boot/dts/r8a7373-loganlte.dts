/dts-v1/;
/include/ "r8a7373.dtsi"

/{
	model = "LoganLTE";
	compatible = "renesas,loganlte", "renesas,r8a7373";

	chosen {
		bootargs = "console=ttySC0,115200 earlyprintk=sh-sci.0,115200 ignore_loglevel";
	};

	memory@49000000 {
		device_type = "memory";
		reg = <0x49000000 0x37000000>;
	};
 };
