# tmu.mk


#----------------------------------------------------------------
# target
#----------------------------------------------------------------
TARGET=tmu
ULIB = $(TARGET).a

#----------------------------------------------------------------
# include and symbol path
#----------------------------------------------------------------
INC0    = ../src
INC1    = ../inc
INC2    = ../../../inc
INC     = -I$(INC0) -I$(INC1) -I$(INC2)
VPATH   = $(INC0):$(INC1):$(INC2)

#----------------------------------------------------------------
# option
#----------------------------------------------------------------
VIA_DIR = ../../../via
OPT     = $(VIA_DIR)/loader_opt.mk

include $(OPT)
#----------------------------------------------------------------
# object
#----------------------------------------------------------------
OBJS    = tmu_api.o timer_drv.o 

.PHONY : allclean clean all
all: $(ULIB)
#----------------------------------------------------------------
# dependence
#----------------------------------------------------------------
$(OBJS)             : $(TARGET).mk $(OPT)
timer_drv.o         : lcd_common.h timer_drv.h
tmu_api.o           : timer_drv.h tmu_api.h 

#----------------------------------------------------------------
# LINK START
#----------------------------------------------------------------

include $(VIA_DIR)/loader_lib.mk
